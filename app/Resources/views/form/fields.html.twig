{% block form_row %}
	<div class="form-group">
	    <div class="col-sm-6">
	    	{{ form_label(form) }}	    	
	    </div>

	    <div class="col-sm-10">
	    	{{ form_widget(form, {'attr':{'class':'form-control'} } ) }}
	    	<span style="color:red;"> {{ form_errors(form)  }} </span>
	    </div>
	</div>
{% endblock %}
